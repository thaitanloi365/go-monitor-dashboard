{
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "UMI_ENV=local PORT=9998 node_modules/umi/bin/umi.js dev",
    "prod": "UMI_ENV=prod node_modules/umi/bin/umi.js dev",
    "staging": "UMI_ENV=staging node_modules/umi/bin/umi.js dev",
    "build": "UMI_ENV=local node_modules/umi/bin/umi.js build",
    "build-dev": "UMI_ENV=dev node_modules/umi/bin/umi.js build",
    "build-prod": "UMI_ENV=prod node_modules/umi/bin/umi.js build",
    "build-staging": "NODE_OPTIONS=--max_old_space_size=4096 UMI_ENV=staging node_modules/umi/bin/umi.js build",
    "test": "umi test",
    "lint:es": "eslint --ext .js src mock tests",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@ant-design/compatible": "0.0.1-rc.1",
    "ant-design-pro": "2.3.2",
    "axios": "0.19.2",
    "classnames": "2.2.6",
    "dva-model-extend": "0.1.2",
    "enquire-js": "^0.2.1",
    "moment-timezone": "0.5.31",
    "nprogress": "0.2.0",
    "qs": "6.9.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "react-perfect-scrollbar": "1.5.8",
    "react-timeago": "4.4.0",
    "store": "2.0.12"
  },
  "devDependencies": {
    "@types/classnames": "2.2.9",
    "@types/jest": "25.1.3",
    "@types/nprogress": "0.2.0",
    "@types/qs": "6.9.1",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.1.0",
    "@types/react-router-dom": "5.1.5",
    "@types/react-test-renderer": "16.9.2",
    "@types/react-timeago": "4.1.1",
    "@types/store": "2.0.2",
    "babel-eslint": "10.0.3",
    "dva-logger": "1.0.0",
    "eslint": "6.8.0",
    "eslint-config-umi": "1.6.0",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.3",
    "grunt-bump": "0.8.0",
    "husky": "4.2.3",
    "lint-staged": "10.0.7",
    "react-test-renderer": "16.12.0",
    "tslint": "6.0.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.2.0",
    "umi": "^2",
    "umi-plugin-react": "1.15.8",
    "umi-types": "0.5.14"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
